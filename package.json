{
  "name": "polymer-lit-playground",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@material/button": "^0.37.1",
    "@material/card": "^0.37.1",
    "@material/checkbox": "^0.37.1",
    "@material/drawer": "^0.37.1",
    "@material/image-list": "^0.37.1",
    "@material/list": "^0.37.1",
    "@material/radio": "^0.37.1",
    "@material/select": "^0.37.1",
    "@material/switch": "^0.36.1",
    "@material/tabs": "^0.37.1",
    "@material/textfield": "^0.37.1",
    "@material/top-app-bar": "^0.37.1",
    "@material/mwc-button": "^0.1.2",
    "@material/mwc-radio": "^0.1.2",
    "@material/mwc-switch": "^0.1.2",
    "@polymer/lit-element": "^0.5.2",
    "@polymer/polymer": "^3.0.2",
    "@webcomponents/webcomponentsjs": "^2.0.2"
  },
  "devDependencies": {
    "browser-sync": "2.24.5",
    "node-sass": "4.9.2",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.3",
    "polyserve": "0.27.12",
    "ts-node": "7.0.0",
    "typescript": "2.9.2"
  },
  "scripts": {
    "start:dev": "npm-run-all --parallel serve devWatch devWatchCss",
    "devWatch": "npm-run-all --serial utilDelay serveWatch",
    "devWatchCss": "npm-run-all --serial utilDelay serveWatchCss",
    "serve": "polyserve --port $npm_package_config_servePort --npm",
    "serveWatch": "browser-sync start --port $npm_package_config_serveWatchPort --proxy \"localhost:$npm_package_config_servePort\" --no-ui --files '*.js, *.html, demo/**/*.html, demo/**/*.css, demo/**/*.scss' --startPath \"/demo/mdc-101-web.html\"",
    "serveWatchCss": "node-sass --watch --include-path node_modules/ --output demo/ --recursive demo/",
    "utilDelay": "node ./node_modules/.bin/npm-delay 1000"
  },
  "config": {
    "servePort": 9030,
    "serveWatchPort": 9040
  }
}
